<!-- styles -->
<script>
  (function() {
	function loadCSS(css) {
		var timeout = window.requestAnimationFrame || window.setTimeout;
		var tag = document.createElement('link');
		tag.rel = 'stylesheet';
		tag.href = css;
		timeout(function(){
			document.head.appendChild(tag);
		});
	}
	loadCSS("https://cdn.jsdelivr.net/gh/xcodebuild/blogsite@master/themes/cactus/source/lib/font-awesome/css/all.min.css");
	loadCSS("https://cdn.jsdelivr.net/gh/xcodebuild/blogsite@master/themes/cactus/source/lib/justified-gallery/css/justifiedGallery.min.css");
  })()
</script>
